        "aws/aws-sdk-php": "^3.20",
        "guzzlehttp/guzzle": "~6.0",
        "vinceg/usps-php-api": "^1.0",
        "hybridauth/hybridauth": "3.0.x-dev",
        "smalot/pdfparser": "^0.10.0",
