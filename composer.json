            }
        }
    ],
    "require": {
        "php": ">=5.4",
        "ext-phalcon": ">=2.0.9",
        "phalcon/devtools": "dev-master",
        "chrisboulton/php-resque": "1.2.x",
        "cpigroup/php-amazon-mws": "1.3.0",
        "jeremy-dunn/php-fedex-api-wrapper": "dev-master",
        "gabrielbull/ups-api": "dev-master",
        "pda/pheanstalk": "^3.1",
