{
    "repositories": [
        {
            "type":"package",
            "package": {
              "name": "ShippingEasy/shipping_easy-php",
              "version":"1.1",
              "source": {
                  "url": "https://github.com/ShippingEasy/shipping_easy-php.git",
                  "type": "git",
                  "reference":"master"
